{
  "name": "nextjs-boilerplate",
  "version": "0.0.1-5",
  "private": true,
  "scripts": {
    "dev": "yarn env:copy:dev && next dev",
    "dev:qa": "yarn env:copy:qa && next dev",
    "build": "yarn env:copy:dev && next build",
    "build:qa": "yarn env:copy:qa && next build",
    "build:production": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "env:copy:dev": "cp .env.development .env.local",
    "env:copy:qa": "cp .env.qa .env.local",
    "release:pre": "yarn version --prerelease && yarn extract",
    "release:patch": "yarn version --patch && yarn extract",
    "release:minor": "yarn version --minor && yarn extract",
    "release:major": "yarn version --major && yarn extract",
    "extract": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts && yarn format:fix && git add . && git commit -m 'build: version update' && git push --tags",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.5",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-three/drei": "^10.0.7",
    "@react-three/fiber": "^9.1.2",
    "@tanstack/react-query": "^5.66.0",
    "@types/three": "^0.176.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "decimal.js": "^10.5.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.9.2",
    "input-otp": "^1.4.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.487.0",
    "motion": "^12.9.2",
    "next": "15.1.6",
    "next-themes": "^0.4.6",
    "react": "19.0.0",
    "react-day-picker": "8.10.1",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "react-icon-cloud": "^4.1.7",
    "react-icons": "^5.5.0",
    "react-parallax-mouse": "^2.1.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^2.15.2",
    "sharp": "^0.33.5",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.176.0",
    "vaul": "^1.1.2",
    "yup": "^1.6.1",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.3",
    "@tailwindcss/postcss": "^4.1.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/eslint-plugin-query": "^5.66.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "20.14.2",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "8.43.0",
    "eslint-config-next": "15.1.6",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^4.1.3",
    "typescript": "5.7.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": "Unsupported. Use Yarn instead.",
    "yarn": ">= 1.22.00",
    "node": ">= 18.17.0"
  },
  "packageManager": "yarn@4.6.0",
  "resolutions": {
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3"
  }
}
